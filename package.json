{
  "name": "hopperse",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "seed": "dotenv -e .env.development.local -- npx prisma db seed",
    "generate": "dotenv -e .env.development.local -- npx prisma generate",
    "prisma": "dotenv -e .env.development.local npx prisma db pull && npx prisma generate",
    "push": "dotenv -e .env.development.local -- npx prisma db push",
    "lint-staged": "lint-staged",
    "format": "prettier --write \"**/*.{ts, tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "check": "prettier --check \"**/*.{ts, tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "prepare": "[ -n \"$CI\" ] && echo 'Skipping husky in CI environment' || npx husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup-vercel": "node scripts/setup-vercel.mjs",
    "deploy:dev": "vercel --env NODE_ENV=development",
    "deploy:staging": "vercel --env NODE_ENV=staging",
    "deploy:prod": "vercel --prod",
    "vercel-build": "vercel build",
    "vercel-build:debug": "NEXT_DEBUG_BUILD=true vercel build",
    "check-paths": "node scripts/check-path-aliases.mjs",
    "fix-paths": "node scripts/fix-path-aliases.mjs"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
